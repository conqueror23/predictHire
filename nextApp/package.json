{
  "name": "front-end",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "add-mockup": "mongoimport --host localhost --port 27018 --username root --password root --authenticationDatabase admin --db predictiveHire-company  --collection company --type json --file ./mockup/company.json --jsonArray && mongoimport --host localhost --port 27018 --username root --password root --authenticationDatabase admin --db predictiveHire-user  --collection user  --file ./mockup/users.json --jsonArray",
    "dev": "npm run add-mockup && next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "pretty": "prettier --write \"./**/*.{js,jsx,json}\""
  },
  "dependencies": {
    "@apollo/client": "^3.4.12",
    "@shelf/jest-mongodb": "^2.0.3",
    "apollo-server-micro": "^2.25.2",
    "axios": "^0.21.4",
    "bcryptjs": "^2.4.3",
    "express": "^4.17.1",
    "graphql": "^15.5.3",
    "jsonwebtoken": "^8.5.1",
    "next": "11.1.2",
    "prettier": "^2.4.1",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-typescript": "^7.15.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.2",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/react": "17.0.21",
    "eslint": "7.32.0",
    "eslint-config-next": "11.1.2",
    "jest": "^27.2.1",
    "ts-jest": "^27.0.5",
    "typescript": "4.4.3"
  }
}
